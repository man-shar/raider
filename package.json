{
  "name": "raider",
  "version": "1.0.0",
  "description": "Reader + AI = Raider",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@aarkue/tiptap-math-extension": "^1.3.4",
    "@anthropic-ai/sdk": "^0.39.0",
    "@defogdotai/agents-ui-components": "^1.8.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@floating-ui/react": "^0.27.4",
    "@google/generative-ai": "^0.24.0",
    "@tailwindcss/vite": "^4.0.0",
    "@tippyjs/react": "^4.2.6",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "better-sqlite3": "^11.8.1",
    "dmg-builder": "26.0.0",
    "electron-updater": "^6.3.9",
    "katex": "^0.16.21",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.474.0",
    "openai": "^4.80.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-pdf": "^9.2.1",
    "sqlite-vec": "0.1.7-alpha.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0",
    "tiktoken": "^1.0.18",
    "tiptap-markdown": "^0.8.10"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0-beta.0",
    "@electron-toolkit/eslint-config-ts": "3.0.0-beta.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/rebuild": "^3.7.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/better-sqlite3": "^7.6.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^34.0.1",
    "electron-builder": "^26.0.0",
    "electron-builder-squirrel-windows": "26.0.0",
    "electron-vite": "3.0.0-beta.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "install": "^0.13.0",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.83.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  }
}
